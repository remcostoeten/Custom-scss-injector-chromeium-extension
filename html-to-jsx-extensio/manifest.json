{
  "manifest_version": 3,
  "name": "HTML to JSX Converter",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "/images/icon48.png"
    }
  },
  "icons": {
    "48": "/images/icon48.png"
  },
  "permissions": ["activeTab"],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
